@model AdminDashboard.Models.Category

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Category</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ID
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.category_Id)
        </dd>
        <dt class="col-sm-2">
           Category Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.category_name)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.category_Id" class="btn btn-outline-success">Edit</a>
    <a asp-action="Index" class="btn btn-outline-info">Back to List</a>
</div>
